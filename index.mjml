      </mj-column>
    </mj-section>

    <mj-raw>
      <!-- Pickup -->
    </mj-raw>
    <mj-section padding="25px" css-class="bg-white">
      <mj-text font-size="16px">
        <b>Windy City Chicago Style Pizza</b>
        <p style="font-size: 20px; color: #373737">Total: $9.00</p>
      </mj-text>
      <mj-spacer height="1px" />
      <mj-text line-height=".75" font-size="14px" color="#ABABB9">
        <p>Ordered: Sep 20, 2023 11:12:52am</p>
        <p>Order number: #31082380-8076724 </p>
        <p>Contact Restaurant: <a class="link1" href="#" alt="phone number">(725) 205-0290</a></p>
