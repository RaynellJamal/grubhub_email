      </mj-column>

      <mj-column>
        <mj-image src="https://i.ibb.co/jWw7Vxb/staticmap.png" alt="map" width="350px" />
      </mj-column>
    </mj-section>

    <mj-raw>
      <!-- Divider -->
    </mj-raw>
    <mj-section align="center" padding="0" css-class="bg-white">
      <mj-column width="600px">
        <mj-divider border-width="1px" border-color="#CACACA" />
